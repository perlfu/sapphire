		<!-- Flag navigations -->
    <div class="navflag-container">
      <div class="navflag">
			  <ul>
          <li><a href="#"><img class="img-navflag" src="./img/flag_uk.png" alt="" /></a></li>
          <li><a href="#"><img class="img-navflag" src="./img/flag_germany.png" alt="" /></a></li>
          <li><a href="#"><img class="img-navflag" src="./img/flag_sweden.png" alt="" /></a></li>
        </ul>
		  </div>
    </div>			

    <!-- Navigation Level 1 -->
    <div class="nav1-container">
      <div class="nav1">
			  <ul>
$!{quicklinksFragment}
        </ul>
		  </div>
    </div>			

    <!-- Sitename -->
		<div class="site-name">
      <p class="title"><a href="/">${space.name}</a></p>
      #if($shortDescription)
      <p class="subtitle"><a href="/">$!{shortDescription}</a></p>
      #end
    </div>
    
    <!-- Site slogan -->
		<div class="site-slogan-container">
			<div class="site-slogan">
      	<p class="title"><img alt="Codehaus" src="http://media.codehaus.org/images/codehaus-small.gif"/></p>
      </div>
		</div>
    
    <!-- Header banner -->		 						    		 						
	<div>#if($site.resolveProperty("logo.url"))<img class="img-header" src="${site.resolveProperty("logo.url")}" alt="${space.name}" class="spaceLogo" border="0"/>#else<img class="img-header" alt=""/>#end</div>
    
    <!-- Navigation Level 2 -->												
	  <div class="nav2">			
		<ul>
        <li>#if ($page.pageKey != "Home")
                   #foreach($breadcrumb in $breadcrumbs)
                    &gt; <a href="/${breadcrumb.pageKey}">${breadcrumb.pageKey}</a>
                   #end
             #end
            <a href="/">Home</a>
         </li>
<li><a href="http://xircles.codehaus.org/projects/${site.resolveProperty("project.key")}" label="Codehaus project management system">Xircles</a></li>
		  </ul>
		</div>
    
    <!-- Buffer after header -->    

		<!-- NAVIGATION -->				
  	<!-- Navigation Level 3 -->
		<div class="nav3">
      	<ul>
	  #if (${navigationFragment})
            ${navigationFragment}
          #end
