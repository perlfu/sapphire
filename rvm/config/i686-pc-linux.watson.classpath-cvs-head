. $RVM_ROOT/rvm/config/i686-pc-linux-gnu

export CLASSPATH_ROOT=@reset-by-configuration-script@
export RVM_WITH_CLASSPATH_CVS_HEAD=1
export JIKES=/homes/meriadoc/jalapeno/bin/i686/jikes-1.22
export ANT_CMD=/homes/meriadoc/jalapeno/contrib/jakarta-ant-1.4.1/bin/ant
export BENCHMARK_ROOT=/homes/meriadoc/jalapeno/contrib/benchmarks
