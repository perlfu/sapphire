#!/bin/ksh
#
# (C) Copyright IBM Corp. 2001
#
#$Id$
#
# Produce a ".java" file from a "VM_Dictionary.template" file.
#
# @author Bowen Alpern
# @author Anthony Cocchi
# @author Derek Lieber

if [ $# != 7 ]; then
   echo "usage: $0 packageName dictionaryClassName keyTypeName valueTypeName hasherClassName templateFileName outputFileName"
   exit 1
fi

PNAME=$1
CNAME=$2
KNAME=$3
VNAME=$4
DNAME=$5
IFILE=$6
OFILE=$7

echo '/* THIS FILE IS MACHINE GENERATED FROM "'$IFILE'" -- DO NOT EDIT */'           >$OFILE
echo                                                                                >>$OFILE
sed -e s/PPP/$PNAME/g -e s/CCC/$CNAME/g -e s/KKK/$KNAME/g -e s/VVV/$VNAME/g -e s/DDD/$DNAME/g <$IFILE >>$OFILE
