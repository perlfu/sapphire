# This script extracts the best running time from a
# SPEC output file called "tmp"

eval "exec perl $0 $*"
	if 0;
open(INPUT,"tmp");
$minTime=100000;
while ($line=<INPUT>) {
   ($junk1,$junk2,$junk3,$junk4,$time,$junk5) = split(/ /,$line);
   if ($time < $minTime) { $minTime = $time; }
}
close(INPUT);
print "$minTime";
